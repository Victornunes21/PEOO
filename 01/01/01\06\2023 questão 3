lado1 = float(input("informe o promeiro lado: "))
lado2 = float(input("Irforme o segundo lado: "))
lado3 = float(input("Informe o terceiro lado: "))

if lado1 == lado2 and lado2 == lado3 and lado3 == lado1:
    print("Esse triângulo é IQUELATERO!")
elif lado2 == lado1  or lado2 == lado3 and lado1 != lado3 or lado3 == lado1 and lado2 != lado1:
    print("Esse triâmgulo é ISOSELES!")
if lado1 != lado2 and lado2 != lado3 and lado3 != lado1:
    print("Esse triângulo é ESCALENO!")
    
